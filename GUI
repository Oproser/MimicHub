	--]]

		local PageLibrary = {}

		function PageLibrary.AddButton(Text, Side, Callback)

			local Parent
			if Side == "Left" then Parent = ButtonRow1 elseif Side == "Right" then Parent = ButtonRow2 end

			local Button = Instance.new("Frame")
			Button.Name = "Button"
			Button.Parent = Parent
			Button.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
			Button.BackgroundTransparency = 1.000
			Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Button.Position = UDim2.new(0.0199999325, 0, 0.0799999088, 0)
			Button.Size = UDim2.new(0.940799892, 0, 0.115887977, 0)

			local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
			UIAspectRatioConstraint.Parent = Button
			UIAspectRatioConstraint.AspectRatio = 13.023

			local MainButton = Instance.new("Frame")
			MainButton.Name = "MainButton"
			MainButton.Parent = Button
			MainButton.BackgroundColor3 = Color3.fromRGB(79, 79, 79)
			MainButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
			MainButton.Size = UDim2.new(0.0789705887, 0, 0.973689079, 0)

			local Inside = Instance.new("ImageButton")
			Inside.Name = "Inside"
			Inside.Parent = MainButton
			Inside.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
			Inside.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Inside.Position = UDim2.new(0.150000006, 0, 0.150000006, 0)
			Inside.Size = UDim2.new(0.699999988, 0, 0.699999988, 0)
			Inside.Image = "rbxassetid://0"

			local TextLabel_2 = Instance.new("TextLabel")
			TextLabel_2.Parent = Button
			TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel_2.BackgroundTransparency = 1.000
			TextLabel_2.BorderColor3 = Color3.fromRGB(27, 42, 53)
			TextLabel_2.Position = UDim2.new(0.0671254843, 0, 0, 0)
			TextLabel_2.Size = UDim2.new(0.330985665, 0, 0.973689079, 0)
			TextLabel_2.Font = Enum.Font.GothamSemibold
			TextLabel_2.Text = Text
			TextLabel_2.TextColor3 = Color3.fromRGB(170, 170, 170)
			TextLabel_2.TextSize = 9.000
			TextLabel_2.TextYAlignment = Enum.TextYAlignment.Bottom



			function Button()
				local Color = true
				Inside.MouseButton1Down:Connect(function()
					if Color == true then
						Color = not Color
						Inside.BackgroundColor3 = Color3.fromRGB(170,170,170)
					else
						Color = not Color
						Inside.BackgroundColor3 = Color3.fromRGB(46,46,46)
					end
					Callback()
				end)
			end
			coroutine.wrap(Button)()
		end
