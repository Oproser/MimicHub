		function TabChange()
			TabButton.MouseButton1Click:Connect(function()
				print("working")
				Page.Visible = true
				Cool2.Visible = true
				for i,v in pairs(Main:GetChildren()) do
					if v ~= Page and v.ZIndex == 5 then
						v.Visible = false
					end
				end
				for i,v in pairs(Menus:GetDescendants()) do
					if v ~= Cool2 and v:IsA("Frame") then
						v.Visible = false
					end
				end
				TabButton.BackgroundColor3 = Color3.fromRGB(46, 46, 46) 
			end)
		end
		coroutine.wrap(TabChange)()


	--[[
