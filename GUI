	|   |   |   |   |   |   |   |   |   |
	--]]

		function PageLibrary.AddSlider(Text, Side, ConfigurationDictionary, Callback)

			local Parent
			if Side == "Left" then Parent = ButtonRow1 elseif Side == "Right" then Parent = ButtonRow2 end

			local Configuration = ConfigurationDictionary
			local Minimum = Configuration.Minimum or Configuration.minimum or Configuration.Min or Configuration.min
			local Maximum = Configuration.Maximum or Configuration.maximum or Configuration.Max or Configuration.max
			local Default = Configuration.Default or Configuration.default or Configuration.Def or Configuration.def

			if Minimum > Maximum then
				local StoreValue = Minimum
				Minimum = Maximum
				Maximum = StoreValue
			end

			Default = math.clamp(Default, Minimum, Maximum)

			local DefaultScale = Default/Maximum

			local Bar = Instance.new("TextButton")
			Bar.Name = "Bar"
			Bar.Parent = Parent
			Bar.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
			Bar.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Bar.ClipsDescendants = true
			Bar.Position = UDim2.new(0.0199999325, 0, 0.0799999088, 0)
			Bar.Size = UDim2.new(0.940799892, 0, 0.115887977, 0)
			Bar.Text = ""
			Bar.AutoButtonColor = false

			local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
			UIAspectRatioConstraint.Parent = Bar
			UIAspectRatioConstraint.AspectRatio = 13.023

			local Fill = Instance.new("Frame")
			Fill.Name = "Fill"
			Fill.Parent = Bar
			Fill.BorderSizePixel = 0
			Fill.BackgroundColor3 = Color3.fromRGB(79, 79, 79)
			Fill.Size = UDim2.new(DefaultScale, 0, 1, 0)

			local Changing = Instance.new("TextLabel")
			Changing.Name = "Changing"
			Changing.Parent = Bar
			Changing.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Changing.BackgroundTransparency = 1.000
			Changing.Size = UDim2.new(1, 0, 1, 0)
			Changing.Font = Enum.Font.GothamBold
			Changing.Text = Text
			Changing.TextColor3 = Color3.fromRGB(170, 170, 170)
			Changing.Text = Text..": "..tostring(Default)

			local dragging = false
			Bar.MouseButton1Down:Connect(function()
				dragging = true
			end)
			UserInputService.InputChanged:Connect(function()
				if dragging == true then
					local X, Y, XScale, YScale = GetXY(Bar)
					local Value = math.floor(Minimum + ((Maximum - Minimum) * XScale))
					Callback(Value)
					Changing.Text = Text..": "..tostring(Value)
					local TargetSize = UDim2.new(XScale,0,1,0)
					Tween(Fill, {Size = TargetSize})
					local SliderMove, SliderKill
					SliderMove = Mouse.Move:Connect(function()
						if dragging == true then
							local X, Y, XScale, YScale = GetXY(Bar)
							local Value = math.floor(Minimum + ((Maximum - Minimum) * XScale))
							Callback(Value)
							Changing.Text = Text..": "..tostring(Value)
							local TargetSize = UDim2.new(XScale,0,1,0)
							Tween(Fill, {Size = TargetSize})
						end
					end)
				end
			end)
			SliderKill = UserInputService.InputEnded:Connect(function(UserInput)
			
				if UserInput.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = false
					Tween(Fill, {ImageTransparency = 0.7})
					SliderMove:Disconnect()
					SliderKill:Disconnect()
				end
			end)
		end

	--[[
	|   |   |   |   |   |   |   |   |   |
	|   |   |   |   |   |   |   |   |   |
	|   |   |   |   |   |   |   |   |   |
	--]]
		function PageLibrary.AddInputButton(Text, Side, Callback)
			local Parent
			if Side == "Left" then Parent = ButtonRow1 elseif Side == "Right" then Parent = ButtonRow2 end
			local Color = true
			local selecting = false
			local keyn

			local InputButton = Instance.new("Frame")
			InputButton.Name = "InputButton"
			InputButton.Parent = Parent
			InputButton.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
			InputButton.BackgroundTransparency = 1.000
			InputButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
			InputButton.Position = UDim2.new(0.0199999325, 0, 0.0799999088, 0)
			InputButton.Size = UDim2.new(0.940799892, 0, 0.115887977, 0)

			local UIAspectRatioConstraint_6 = Instance.new("UIAspectRatioConstraint")
			UIAspectRatioConstraint_6.Parent = InputButton
			UIAspectRatioConstraint_6.AspectRatio = 13.023

			local MainButton_2 = Instance.new("Frame")
			MainButton_2.Name = "MainButton"
			MainButton_2.Parent = InputButton
			MainButton_2.BackgroundColor3 = Color3.fromRGB(79, 79, 79)
			MainButton_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
			MainButton_2.Size = UDim2.new(0.0789705887, 0, 0.973689079, 0)

			local Inside_2 = Instance.new("ImageButton")
			Inside_2.Name = "Inside"
			Inside_2.Parent = MainButton_2
			Inside_2.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
			Inside_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Inside_2.Position = UDim2.new(0.150000006, 0, 0.150000006, 0)
			Inside_2.Size = UDim2.new(0.699999988, 0, 0.699999988, 0)
			Inside_2.Image = "rbxassetid://0"

			local TextLabel_3 = Instance.new("TextLabel")
			TextLabel_3.Parent = InputButton
			TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel_3.BackgroundTransparency = 1.000
			TextLabel_3.BorderColor3 = Color3.fromRGB(27, 42, 53)
			TextLabel_3.Position = UDim2.new(0.0671254843, 0, 0, 0)
			TextLabel_3.Size = UDim2.new(0.330985665, 0, 0.973689079, 0)
			TextLabel_3.Font = Enum.Font.GothamSemibold
			TextLabel_3.Text = Text
			TextLabel_3.TextColor3 = Color3.fromRGB(170, 170, 170)
			TextLabel_3.TextSize = 9.000
			TextLabel_3.TextYAlignment = Enum.TextYAlignment.Bottom

			local InputBut = Instance.new("TextButton")
			InputBut.Parent = InputButton
			InputBut.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			InputBut.BackgroundTransparency = 1.000
			InputBut.Position = UDim2.new(0.648000002, 0, 0, 0)
			InputBut.Size = UDim2.new(0.330000013, 0, 1, 0)
			InputBut.Font = Enum.Font.SourceSans
			InputBut.Text = "[ . . . ]"
			InputBut.TextColor3 = Color3.fromRGB(170, 170, 170)
			InputBut.TextScaled = true
			InputBut.TextSize = 11.000
			InputBut.TextWrapped = true


			function InputButton()
				InputBut.MouseButton1Click:Connect(function(input, gameProcessed)
				if gameProcessed then return end
					selecting = false
					if selecting == false then
						InputBut.Text = "[ . . . ]"
						UserInputService.InputBegan:Connect(function(input, gameProcessed)
							if selecting == false then
								local key = string.split(tostring(input.KeyCode), ".")
								InputBut.Text = "[ " .. key[3] .. " ]"
								keyn = input.KeyCode
								wait(0.1)
								selecting = true
							end
						end)
					end
				end)
				UserInputService.InputBegan:Connect(function(input, gameProcessed)
				if gameProcessed then return end
					if selecting == true then
						if input.KeyCode == keyn then
							if Color == true then
								Color = not Color
								Inside_2.BackgroundColor3 = Color3.fromRGB(170,170,170)
							else
								Color = not Color
								Inside_2.BackgroundColor3 = Color3.fromRGB(46,46,46)
							end
							Callback()
						end
					end
				end)
				Inside_2.MouseButton1Down:Connect(function(i,gameProcessed)
					if Color == true then
						Color = not Color
						Inside_2.BackgroundColor3 = Color3.fromRGB(170,170,170)
					else
						Color = not Color
						Inside_2.BackgroundColor3 = Color3.fromRGB(46,46,46)
					end
					Callback()
				end)
			end
			coroutine.wrap(InputButton)()
		end

		function PageLibrary.AddSpacer(Text, Side)

			local Parent
			if Side == "Left" then Parent = ButtonRow1 elseif Side == "Right" then Parent = ButtonRow2 end

			local intName = Instance.new("Frame")
			intName.Name = "idk"
			intName.Parent = Parent
			intName.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
			intName.BackgroundTransparency = 1.000
			intName.BorderColor3 = Color3.fromRGB(0, 0, 0)
			intName.Position = UDim2.new(0.02, 0, 0.08, 0)
			intName.Size = UDim2.new(0.95, 0, 0.1, 0)

			local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
			UIAspectRatioConstraint.Parent = intName
			UIAspectRatioConstraint.AspectRatio = 13.023

			local Frame = Instance.new("Frame")
			Frame.Parent = intName
			Frame.BackgroundColor3 = Color3.fromRGB(79, 79, 79)
			Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Frame.Position = UDim2.new(0.0199999996, 0, 0.449999988, 0)
			Frame.Size = UDim2.new(0.949999988, 0, 0.0700000003, 0)

			local HeaderText = Instance.new("TextLabel")
			HeaderText.Name = "SpacerText"
			HeaderText.Parent = intName
			HeaderText.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
			HeaderText.BorderSizePixel = 0
			HeaderText.Position = UDim2.new(0.210588217, 0, -0.137717411, 0)
			HeaderText.Size = UDim2.new(0.57391119, 0, 1.16228127, 0)
			HeaderText.ZIndex = 10
			HeaderText.Font = Enum.Font.GothamSemibold
			HeaderText.Text = Text
			HeaderText.TextColor3 = Color3.fromRGB(170, 170, 170)
			HeaderText.TextSize = 10.000

		end

		return PageLibrary

	end
	return TabLibrary
end
return UILibrary
